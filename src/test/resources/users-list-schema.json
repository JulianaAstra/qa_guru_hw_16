{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "users",
  "type": "object",
  "properties": {
    "page": {"type": "integer"},
    "per_page": {"type": "integer"},
    "total": {"type": "integer"},
    "total_pages": {"type": "integer"},
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {"type": "integer"},
          "email": {"type": "string"},
          "first_name": {"type": "string"},
          "last_name": {"type":  "string"},
          "avatar": {"type": "string"}
        },
        "required": ["id", "email"]
      },
      "minItems": 1,
      "uniqueItems": true
    }
  },
  "required": ["page", "per_page", "total", "total_pages", "data"]
}